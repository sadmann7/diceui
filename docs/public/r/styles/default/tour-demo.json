      "content": "\"use client\";\n\nimport * as React from \"react\";\nimport { Button } from \"@/components/ui/button\";\nimport {\n  Tour,\n  TourClose,\n  TourDescription,\n  TourFooter,\n  TourHeader,\n  TourNext,\n  TourOverlay,\n  TourPrev,\n  TourSkip,\n  TourStep,\n  TourStepCounter,\n  TourTitle,\n} from \"@/registry/default/ui/tour\";\n\nexport default function TourDemo() {\n  const [open, setOpen] = React.useState(false);\n\n  return (\n    <div className=\"flex min-h-[400px] flex-col items-center justify-center gap-8 p-8\">\n      <div className=\"flex flex-col items-center gap-4\">\n        <h1 id=\"welcome-title\" className=\"font-bold text-2xl\">\n          Welcome to Our App\n        </h1>\n        <p className=\"text-center text-muted-foreground\">\n          Click the button below to start the tour and learn about our features.\n        </p>\n        <Button id=\"start-tour-btn\" onClick={() => setOpen(true)}>\n          Start Tour\n        </Button>\n      </div>\n\n      <div className=\"grid grid-cols-3 gap-4\">\n        <div id=\"feature-1\" className=\"rounded-lg border p-4 text-center\">\n          <h3 className=\"font-semibold\">Feature 1</h3>\n          <p className=\"text-muted-foreground text-sm\">\n            This is our first amazing feature\n          </p>\n        </div>\n        <div id=\"feature-2\" className=\"rounded-lg border p-4 text-center\">\n          <h3 className=\"font-semibold\">Feature 2</h3>\n          <p className=\"text-muted-foreground text-sm\">\n            This is our second great feature\n          </p>\n        </div>\n        <div id=\"feature-3\" className=\"rounded-lg border p-4 text-center\">\n          <h3 className=\"font-semibold\">Feature 3</h3>\n          <p className=\"text-muted-foreground text-sm\">\n            This is our third incredible feature\n          </p>\n        </div>\n      </div>\n      <Tour\n        open={open}\n        onOpenChange={setOpen}\n        stepFooter={\n          <TourFooter>\n            <div className=\"flex w-full items-center justify-between\">\n              <TourStepCounter />\n              <div className=\"flex gap-2\">\n                <TourPrev />\n                <TourNext />\n              </div>\n            </div>\n          </TourFooter>\n        }\n      >\n        <TourOverlay />\n        <TourStep target=\"#welcome-title\" placement=\"bottom\">\n          <TourHeader>\n            <TourTitle>Welcome!</TourTitle>\n            <TourDescription>\n              This is the main title of our application. Let's explore the\n              features together.\n            </TourDescription>\n          </TourHeader>\n          <TourClose />\n        </TourStep>\n        <TourStep target=\"#feature-1\" placement=\"top\">\n          <TourHeader>\n            <TourTitle>Feature 1</TourTitle>\n            <TourDescription>\n              This is our first feature. It provides amazing functionality that\n              will help you get started.\n            </TourDescription>\n          </TourHeader>\n          <TourClose />\n        </TourStep>\n        <TourStep target=\"#feature-2\" placement=\"top\">\n          <TourHeader>\n            <TourTitle>Feature 2</TourTitle>\n            <TourDescription>\n              Our second feature builds upon the first and adds even more value\n              to your experience.\n            </TourDescription>\n          </TourHeader>\n          <TourClose />\n        </TourStep>\n        <TourStep target=\"#feature-3\" placement=\"top\" required>\n          <TourHeader>\n            <TourTitle>Feature 3</TourTitle>\n            <TourDescription>\n              The final feature in our tour showcases the full potential of our\n              platform. This step is required and cannot be skipped.\n            </TourDescription>\n          </TourHeader>\n          <TourFooter>\n            <div className=\"flex w-full items-center justify-between\">\n              <TourStepCounter />\n              <div className=\"flex gap-2\">\n                <TourSkip />\n                <TourNext />\n              </div>\n            </div>\n          </TourFooter>\n          <TourClose />\n        </TourStep>\n      </Tour>\n    </div>\n  );\n}\n",
      "type": "registry:example",
      "target": ""
    }
  ]
